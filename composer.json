{
	"name": "lasserafn/php-string-script-language",
	"description": "Detect language/encoding of a string in PHP",
	"keywords": [
		"string",
		"language",
		"php"
	],
	"type": "library",
	"license": "MIT",
	"authors": [
		{
			"name": "Lasse Rafn",
			"email": "lasserafn@gmail.com"
		}
	],
	"require": {
		"php": "^5.6|^7.0|^7.1"
	},
	"require-dev": {
		"phpunit/phpunit": "^5.7",
		"satooshi/php-coveralls": "^1.0"
	},
	"autoload": {
		"psr-4": {
			"LasseRafn\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Initials\\Tests\\": "tests/"
		}
	},
	"config": {
		"sort-packages": true
	}
}
